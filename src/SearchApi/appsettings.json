{
  "AppSettings": {
    "IdentityServerConfiguration": {
      "Authority": "/account",
      "AuthorizationUrl": "/account/connect/authorize",
      "TokenUrl": "/account/connect/token",
      "OpenIdConfigurationUrl": "/.well-known/openid-configuration"
    },
    "ApiRoutePrefix": ""
  },
  "ApplicationDetails": {
    "ApplicationTypeId": 2
  },
  "DisposableEmails": {
    "BlackListEmails": [ "yopmail.com" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
